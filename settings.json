// Place your settings in this file to overwrite the default settings
{
    "editor.fontFamily": "Fantasque Sans Mono, BF Tiny Hand",
    "workbench.colorTheme": "Sunburst",
    "editor.tabSize": 2,
    "git.confirmSync": false,
    "files.associations": {
        "dockerfile.go": "go",
        "dockerfile_test.go": "go",
        "config": "yaml",
        "*.sql": "sql",
        "*.yaml.template": "helm",
        ".bazelrc": "shellscript",
        "bazel.rc": "shellscript",
        "*.gvpr": "c",
        "*Fastfile": "ruby"
    },
    "shellcheck.exclude": [
        ".bazelrc",
        "bazel.rc"
    ],
    "files.trimTrailingWhitespace": true,
    "editor.formatOnPaste": true,
    "editor.fontWeight": "400",
    "editor.scrollBeyondLastLine": false,
    "editor.rulers": [
        80,
        100
    ],
    "cSpell.ignoreRegExpList": [
        "<?[\\w.\\-+]+@\\w+(\\.\\w+)+>?",
        "<?\\w+(\\.\\w+)+>?"
        // "<?import>?"
    ],
    "cSpell.userWords": [
        "APNS",
        "Bazel",
        "Datadog",
        "Deployer",
        "Devops",
        "GDPR",
        "Istio",
        "Kubernetes",
        "Oplog",
        "PIIs",
        "RESQUE",
        "Repost",
        "Reposted",
        "Reposts",
        "Uploader",
        "VSCO's",
        "airbnb",
        "aspirational",
        "backend",
        "backends",
        "blazingly",
        "bson",
        "byolken",
        "comparator",
        "copypasta",
        "curation",
        "datastore",
        "debuggability",
        "deployboard",
        "diffy",
        "favorited",
        "favoriting",
        "followee",
        "grpc",
        "hostnames",
        "iface",
        "importpath",
        "init",
        "kube",
        "kubectl",
        "kubelet",
        "logrus",
        "metav",
        "musta",
        "nginx",
        "observability",
        "parallelize",
        "preprod",
        "proto",
        "protobuf",
        "protobufs",
        "protos",
        "replayer",
        "repo",
        "repos",
        "rickypai",
        "roadmap",
        "rosco",
        "seasug",
        "sharded",
        "skylib",
        "statter",
        "structs",
        "tcpdump",
        "toolchain",
        "toolchains",
        "truthy",
        "unencrypted",
        "unindexed",
        "users",
        "vsco"
    ],
    "cSpell.enabledLanguageIds": [
        "c",
        "cpp",
        "csharp",
        "go",
        "javascript",
        "javascriptreact",
        "json",
        "latex",
        "markdown",
        "php",
        "plaintext",
        "python",
        "ruby",
        "text",
        "typescript",
        "typescriptreact",
        "yml"
    ],
    "files.insertFinalNewline": true,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "search.useIgnoreFilesByDefault": true,
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/vendor": false,
        "**/tmp": true,
        "**/public/packs": true,
        "**/public/card_images": true,
    },
    "editor.formatOnType": true,
    "window.reopenFolders": "all",
    "window.newWindowDimensions": "maximized",
    "window.nativeTabs": true,
    "yaml.maxValidationIssues": 0,
    "files.eol": "\n",
    "window.zoomLevel": 0,
    "editor.wordWrap": "off",
    "files.exclude": {
        "**/bazel-*": true,
    },
    "sublimeTextKeymap.promptV3Features": true,
    "eslint.run": "onType",
    "eslint.autoFixOnSave": true,
    "eslint.packageManager": "yarn",
    "ruby.rubocop.onSave": false,
    "ruby.lint": {
        "rubocop": true,
        "ruby": {
            "unicode": true // Runs ruby -wc -Ku
        },
    },
    "go.autocompleteUnimportedPackages": true,
    "explorer.confirmDragAndDrop": false,
    "go.testOnSave": true,
    "extensions.ignoreRecommendations": false,
    "go.lintTool": "golint",
    "workbench.editor.showTabs": true,
    "[go]": {
        "editor.insertSpaces": false,
        "editor.formatOnSave": true,
        "editor.buildOnSave": false,
        "editor.testOnSave": false
    },
    "editor.formatOnSave": false,
    "java.errors.incompleteClasspath.severity": "ignore",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "python.jediEnabled": false,
    "telemetry.enableTelemetry": false,
    "go.formatTool": "goimports",
    "go.useLanguageServer": false,
    "[Log]": {
        "editor.wordWrap": "off"
    },
    "bazel.buildifierExecutable": "/Users/ricky/workspace/bin/buildifier",
    "bazel.buildifierFixOnFormat": true
}
